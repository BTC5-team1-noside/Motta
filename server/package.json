    "seed": "knex seed:run --knexfile src/knexfile.js",
    "makeSeed": "knex seed:make --knexfile src/knexfile.js",
    "rollback": "knex migrate:rollback",
    "build": "npm install && npm run migrate && npm run seed"
  },
