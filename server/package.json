  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js",
    "dev": "concurrently \"npm run start\" \"npm run client\" ",
    "client": "npm run dev --prefix ../client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "knex migrate:latest --knexfile src/knexfile.js",
